  </ItemGroup>

  <ItemGroup>
    <Content Include="store_check.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="btn_invite.png" />
    <Resource Include="invited.png" />
    <Resource Include="store_backpack_bg_highlight.png" />
